{
  "initial:before": {
    "loopback#favicon": {
      "params": "$!../client/favicon.ico"
    }
  },
  "initial": {
    "compression": {
      "enabled": false
    },
    "cors": {
      "params": {
        "origin": true,
        "credentials": true,
        "maxAge": 86400
      }
    },
    "cookie-parser": {
      "params": "DecodrRing"
    }
  },
  "session": {},
  "auth": {},
  "parse": {
    "body-parser#json": {
      "params": {
        "limit": "50mb"
      }
    },
    "body-parser#urlencoded": {
      "params": {
        "limit": "50mb",
        "extended": true
      }
    }
  },
  "routes": {
    "loopback#rest": {
      "paths": [
        "${restApiRoot}"
      ]
    }
  },
  "files": {
    "loopback#static": {
      "params": "$!../client"
    }
  },
  "final": {
    "./middleware/error-handler": {}
  },
  "final:after": {
    "strong-error-handler": {
      "params": {
        "debug": true,
        "log": true
      }
    }
  }
}
