
block seo
	title= pageTitle

block content
	.mdc-layout-grid
		.mdc-layout-grid__inner
			.mdc-layout-grid__cell.mdc-layout-grid__cell--span-6.content-padding-top.content

				.page-header
					h1 Testbench byuser
		
				ul
					for source, sourceEndpoint in items
						li= sourceEndpoint + ' - '
							for count,type in source.counts
								= type + ':' + count + ' '
							ul
								for item  in source.items
									li= item.mention + ' ' + item.type + ' ' + item.about
