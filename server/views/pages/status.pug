
block seo
	title.
		Testbench

block content
	.container-fluid.content-padding-top.content

		.page-header
			h1 Testbench
		.row
			.col-xs-12
				if sockets
					strong Open Clients
					ul
						for socket in sockets
							li= socket.currentUser.username
				if connections
					strong Open Friends
					ul
						for connection in connections
							li= connection.key + ' ' + connection.status
					
