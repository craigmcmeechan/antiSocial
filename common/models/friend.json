{
  "name": "Friend",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "strictObjectIDCoercion": true,
  "mixins": {
    "TimeStamp": {
      "createdAt": "createdOn",
      "updatedAt": "updatedOn",
      "required": false,
      "validateUpsert": true
    }
  },
  "indexes": {
    "Friend_remoteEndPoint": {
      "keys": {
        "remoteEndPoint": 1
      }
    }
  },
  "properties": {
    "status": {
      "type": "string"
    },
    "remoteRequestToken": {
      "type": "string"
    },
    "remoteAccessToken": {
      "type": "string"
    },
    "remoteEndPoint": {
      "type": "string"
    },
    "remotePublicKey": {
      "type": "string"
    },
    "remoteUsername": {
      "type": "string"
    },
    "uniqueRemoteUsername": {
      "type": "string"
    },
    "remoteHost": {
      "type": "string"
    },
    "remoteName": {
      "type": "string"
    },
    "localRequestToken": {
      "type": "string"
    },
    "localAccessToken": {
      "type": "string"
    },
    "originator": {
      "type": "boolean"
    },
    "audiences": {
      "type": "array"
    },
    "tags": {
      "type": "array"
    },
    "highWater": {
      "type": "string"
    },
    "keys": {
      "type": "object"
    },
    "online": {
      "type": "boolean"
    },
    "hash": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "user": {
      "type": "belongsTo",
      "model": "MyUser",
      "foreignKey": "userId"
    },
    "newsFeeds": {
      "type": "hasMany",
      "model": "NewsFeedItem",
      "foreignKey": "friendId"
    }
  },
  "acls": [{
    "accessType": "*",
    "property": "*",
    "principalType": "ROLE",
    "principalId": "$everyone",
    "permission": "DENY"
  }, {
    "accessType": "*",
    "property": "*",
    "principalType": "ROLE",
    "principalId": "admin",
    "permission": "ALLOW"
  }],
  "methods": {}
}
