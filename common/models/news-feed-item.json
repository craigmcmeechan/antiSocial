{
  "name": "NewsFeedItem",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "strictObjectIDCoercion": true,
  "mixins": {},
  "indexes": {
    "NewsFeedItem_about": {
      "keys": {
        "about": 1
      }
    }
  },
  "properties": {
    "uuid": {
      "type": "string",
      "required": true
    },
    "type": {
      "type": "string",
      "required": true
    },
    "source": {
      "type": "string"
    },
    "about": {
      "type": "string"
    },
    "details": {
      "type": "object"
    },
    "humanReadable": {
      "type": "string"
    },
    "createdOn": {
      "type": "date",
      "defaultFn": "now"
    },
    "updatedOn": {
      "type": "date",
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {
    "user": {
      "type": "belongsTo",
      "model": "MyUser",
      "foreignKey": "userId"
    },
    "friend": {
      "type": "belongsTo",
      "model": "Friend",
      "foreignKey": "friendId"
    }
  },
  "acls": [{
    "accessType": "*",
    "property": "*",
    "principalType": "ROLE",
    "principalId": "$everyone",
    "permission": "DENY"
  }, {
    "accessType": "*",
    "property": "*",
    "principalType": "ROLE",
    "principalId": "admin",
    "permission": "ALLOW"
  }, {
    "accessType": "EXECUTE",
    "principalType": "ROLE",
    "principalId": "$everyone",
    "permission": "ALLOW",
    "property": "live"
  }],
  "methods": {}
}
