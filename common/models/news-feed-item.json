  },
  "strictObjectIDCoercion": true,
  "mixins": {},
  "properties": {
    "uuid": {
      "type": "string",
      "required": true
    },
    "type": {
      "type": "string",
      "required": true
    },
    "source": {
      "type": "string"
    },
    "about": {
      "type": "string"
    },
    "details": {
      "type": "object"
    },
    "humanReadable": {
      "type": "string"
    },
    "createdOn": {
      "type": "date",
      "defaultFn": "now"
    },
    "updatedOn": {
      "type": "date",
      "defaultFn": "now"
    },
    "originator": {
      "type": "boolean"
    }
