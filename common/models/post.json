{
  "name": "Post",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "strictObjectIDCoercion": true,
  "mixins": {
    "TimeStamp": {
      "createdAt": "createdOn",
      "updatedAt": "updatedOn",
      "required": false,
      "validateUpsert": true
    }
  },
  "indexes": {
    "Post_uuid": {
      "keys": {
        "uuid": 1
      }
    }
  },
  "properties": {
    "uuid": {
      "type": "string"
    },
    "source": {
      "type": "string"
    },
    "about": {
      "type": "string"
    },
    "body": {
      "type": "string",
      "admin": {
        "inputType": "textarea"
      }
    },
    "visibility": {
      "type": ["string"]
    },
    "categories": {
      "type": ["string"]
    },
    "geoDescription": {
      "type": "string"
    },
    "geoLocation": {
      "type": "object"
    },
    "athoritativeEndpoint": {
      "type": "string"
    },
    "versions": {
      "type": ["object"]
    },
    "tags": {
      "type": ["string"]
    },
    "posted": {
      "type": "boolean"
    },
    "autopost": {
      "type": "date"
    },
    "description": {
      "type": "string"
    },
    "shareEndpoint": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "user": {
      "type": "belongsTo",
      "model": "MyUser",
      "foreignKey": "userId"
    },
    "photos": {
      "type": "hasMany",
      "model": "Photo",
      "foreignKey": "postId",
      "through": "PostPhoto"
    },
    "postPhotos": {
      "type": "hasMany",
      "model": "PostPhoto",
      "foreignKey": "postId"
    }
  },
  "acls": [{
    "accessType": "*",
    "property": "*",
    "principalType": "ROLE",
    "principalId": "$everyone",
    "permission": "DENY"
  }, {
    "accessType": "*",
    "property": "*",
    "principalType": "ROLE",
    "principalId": "admin",
    "permission": "ALLOW"
  }],
  "methods": {}
}
